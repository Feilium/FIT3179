{
  "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
  "width": 1000,
  "height": 600,
  "title": "Current India Covid-19 Cases",
  "projection": {
    "type": "equirectangular"
  },
  "layer": [
    {
      "data": {
        "url": "india-states.json",
        "format": {
          "type": "topojson",
          "feature": "IND_adm1"
        }
      },
      "mark": {
        "type": "geoshape"
      }
    }
  ]
}
